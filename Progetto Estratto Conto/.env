# Environment variables for the FastAPI container
# This file is read by Docker Compose (env_file), not by Python directly.

# --- App network settings ---
APP_HOST=0.0.0.0
APP_PORT=8000
LOG_LEVEL=INFO

# --- Paths inside the container (match the volumes in compose.yml) ---
MODEL_PATH=/app/models/latest.joblib
DATA_DIR=/app/data
LOGS_DIR=/app/logs
CONFIG_DIR=/app/config

